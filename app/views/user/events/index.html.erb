<%= render 'user/events/new',event: @event %>
<%= link_to "全削除",destroy_all_events_path,method: :delete,class: "btn btn-danger" %>

<table class="table">
  <% @events.each do |event| %>
    <tr>
      <td><%= event.title %></td>
      <td><%= display_cooking_number(event) %></td>
      <td><%= event.amount %>個</td>
      <td><%= link_to "編集",edit_event_path(event.id) %></td>
    </tr>
  <% end %>
</table>